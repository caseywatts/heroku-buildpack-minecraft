<% open('ngrok.log') do |f| %>
  <% f.lines.select { |line| line.include?("URL:") }.each do |line| %>
    Server available at: <%= line.match(/tcp:\/\/(.+:[0-9]+) /)[1] %>
    <br />
  <% end %>
<% end %>

<h3>Full Ngrok Log</h3>
<% open('ngrok.log') do |f| %>
  <% f.lines.each do |line| %>
    <%= line %>
    <br />
  <% end %>
<% end %>
